#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/it-incubator/websocket_chat/websocket_chat_back/node_modules/is-ci/node_modules:/mnt/d/it-incubator/websocket_chat/websocket_chat_back/node_modules:/mnt/d/it-incubator/websocket_chat/node_modules:/mnt/d/it-incubator/node_modules:/mnt/d/node_modules:/mnt/d/it-incubator/websocket_chat/websocket_chat_back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/it-incubator/websocket_chat/websocket_chat_back/node_modules/is-ci/node_modules:/mnt/d/it-incubator/websocket_chat/websocket_chat_back/node_modules:/mnt/d/it-incubator/websocket_chat/node_modules:/mnt/d/it-incubator/node_modules:/mnt/d/node_modules:/mnt/d/it-incubator/websocket_chat/websocket_chat_back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-ci/bin.js" "$@"
else
  exec node  "$basedir/../is-ci/bin.js" "$@"
fi
